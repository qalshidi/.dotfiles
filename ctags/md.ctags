--langdef=markdown
--langmap=markdown:.md
--langmap=markdown:.markdown
--regex-markdown=/^#[ \t](.*$)/\1/h1,heading,headings/
--regex-markdown=/^##[ \t](.*$)/\1/h2,heading,headings/
--regex-markdown=/^###[ \t](.*$)/\1/h3,heading,headings/
--regex-markdown=/#(\w+)/\1/t,tag,tags/
--regex-markdown=/#(\w+)/\1/t,tag,tags/
--regex-markdown=/\[\[(\w+)\]\]/\1/t,tag,tags/
--regex-markdown=/\[(\w+)\]/\1/a,anchor,anchors/
